<idea-plugin>
  <version>1.0.0</version>
  <idea-version since-build="233" until-build="243.*" />
  <id>br.com.gitflowhelper</id>
  <name>Git Flow Helper</name>
  <vendor>Git Flow Helper</vendor>
  <depends>com.intellij.modules.platform</depends>
  <depends>Git4Idea</depends>
  <extensions defaultExtensionNs="com.intellij">
    <statusBarWidgetFactory id="GitFlowWidget" implementation="br.com.gitflowhelper.statusbar.GitFlowStatusBarWidgetFactory" />
    <toolWindow id="GitFlowOutput" anchor="bottom" icon="com.intellij.icons.ExpUiIcons.General.Vcs" factoryClass="br.com.gitflowhelper.toolwindow.GitFlowToolWindowFactory" />
  </extensions>
</idea-plugin>
